{
	"containerDefinitions": [
		{
			"name": "database",
			"image": "postgres:latest",
			"memory": "512",
			"portMappings": [
				{
					"containerPort": 5432,
					"hostPort": 5432
				}
			]
		},
		{
			"name": "nestjs",
			"image": "810177324512.dkr.ecr.us-east-2.amazonaws.com/taxi-xpecter:latest",
			"cpu": 0,
			"portMappings": [
				{
					"containerPort": 3000,
					"hostPort": 3000
				}
			],
			"essential": true,
			"environment": [],
			"environmentFiles": [],
			"mountPoints": [],
			"volumesFrom": [],
			"ulimits": [],
			"logConfiguration": {
				"logDriver": "awslogs",
				"options": {
					"awslogs-create-group": "true",
					"awslogs-group": "/ecs/role-nestjs",
					"awslogs-region": "us-east-2",
					"awslogs-stream-prefix": "ecs"
				},
				"secretOptions": []
			},
			"dependsOn": [
				{
					"containerName": "database",
					"condition": "START"
				}
			]
		}
	],
	"family": "role-nestjs",
	"taskRoleArn": "arn:aws:iam::810177324512:role/role-nestjs",
	"executionRoleArn": "arn:aws:iam::810177324512:role/role-nestjs",
	"networkMode": "awsvpc",
	"volumes": [],
	"placementConstraints": [],
	"requiresCompatibilities": ["FARGATE"],
	"cpu": "1024",
	"memory": "3072",
	"runtimePlatform": {
		"cpuArchitecture": "X86_64",
		"operatingSystemFamily": "LINUX"
	},
	"tags": []
}
